{
  "name": "api-portaria",
  "version": "1.0.0",
  "main": "index.js",
  "author": "JeanOmeg <jeanomeg3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "dev": "yarn && yarn ts-node-dev --inspect --trace-warnings -r tsconfig-paths/register src/server.ts",
    "db:create": "yarn sequelize db:create",
    "db:migrate": "yarn sequelize db:migrate",
    "db:undo": "yarn sequelize db:migrate:undo",
    "db:undo:all": "yarn sequelize db:migrate:undo:all",
    "lint": "yarn eslint . --ext .ts",
    "start": "yarn db:migrate && yarn ts-node -r tsconfig-paths/register src/server.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/sequelize": "^4.28.17",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "sequelize-cli": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
