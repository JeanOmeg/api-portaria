{
  "name": "api-portaria",
  "version": "1.0.0",
  "main": "index.js",
  "author": "JeanOmeg <jeanomeg3@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "i:portaria": "yarn && yarn sequelize db:create && yarn sequelize db:migrate && yarn sequelize db:seed:all",
    "db:migrate": "yarn sequelize db:migrate",
    "db:seed": "yarn sequelize db:seed:all",
    "db:undo": "yarn sequelize db:migrate:undo",
    "db:undo:all": "yarn sequelize db:migrate:undo:all",
    "db:cm": "yarn sequelize migration:create --name ",
    "db:sd": "yarn sequelize seed:create --name ",
    "build": "tsc -p .",
    "dev": "yarn && yarn eslint . --ext .ts && yarn ts-node-dev --inspect --trace-warnings -r tsconfig-paths/register src/server.ts",
    "start": "yarn && yarn db:migrate && yarn db:migrate && yarn ts-node -r tsconfig-paths/register src/server.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.34.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/sequelize": "^4.28.17",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
